    "celebrate": "13.0.3",
    "cors": "^2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-jwt": "^6.0.0",
    "express-winston": "4.1.0",
    "http-status-codes": "2.1.4",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.3",
    "morgan": "^1.9.1",
