  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.13",
    "@types/express": "4.17.1",
    "@types/jest": "^26.0.4",
    "@types/joi": "17.2.3",
    "@types/node-fetch": "^2.6.4",
    "@types/supertest": "^2.0.10",
    "jest": "^26.1.0",
    "nodemon": "2.0.7",
    "prettier": "2.3.0",
    "rimraf": "^3.0.2",
    "supertest": "6.1.3",
    "ts-jest": "^26.1.3",
    "ts-node": "10.0.0",
    "tsc-alias": "^1.8.6",
