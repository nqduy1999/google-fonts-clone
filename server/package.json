    "~/entities": "src/entities",
    "~/libs": "src/libs",
    "~/middlewares": "src/middlewares",
    "~/models": "src/models",
    "~/routes": "src/routes",
    "~/services": "src/services",
    "~/types": "src/types",
    "~/utilities": "src/utilities"
  }
